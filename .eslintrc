{
    "env":{
        "browser": true,
        "jest": true,
        "node": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "settings": {
        "react": {
            "version": "999.999.999"
        }
    },
    "extends": [ "plugin:react/recommended" ],
    "rules": {
        "comma-dangle": [
            "error", {
                "arrays": "ignore",
                "objects": "only-multiline",
                "imports": "ignore",
                "exports": "ignore",
                "functions": "never"
            }
        ],
        "no-cond-assign": 0,
        "no-console": 0,
        "no-constant-condition": 2,
        "no-debugger": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": 0,
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-unreachable": 2,
        "valid-typeof": 2,
        "block-scoped-var": 0,
        "consistent-return": 2,
        "curly": 2,
        "dot-notation": 2,
        "eqeqeq": [2, "allow-null"],
        "jsx-quotes": [2, "prefer-double"],
        "no-alert": 2,
        "no-eval": 2,
        "no-extra-bind": 2,
        "no-fallthrough": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-multi-spaces": [2, {
            "ignoreEOLComments": true
        }],
        "no-multi-str": 2,
        "no-return-assign": 0,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-unused-expressions": 0,
        "prefer-const": 1,
        "radix": 2,
        "no-undef": 2,
        "indent": [2, 4, {
            "FunctionExpression": {
                "parameters": "off"
            },
            "ignoredNodes": ["CallExpression", "ConditionalExpression"]
        }],
        "comma-spacing": [1, {"before": false, "after": true}],
        "comma-style": [0, "last"],
        "new-cap": [1, {"newIsCap": true, "capIsNew": false}],
        "no-trailing-spaces": 1,
        "no-multiple-empty-lines": [1, {"max": 2}],
        "no-nested-ternary": 0,
        "no-var": 2,
        "max-depth": [0, 3],
        "max-params": [2, 5],
        "react/display-name": 0,
        "react/jsx-boolean-value": 1,
        "react/jsx-closing-bracket-location": 2,
        "react/jsx-curly-spacing": 2,
        "react/jsx-equals-spacing": 2,
        "react/jsx-handler-names": 1,
        "react/jsx-indent": [2, 4],
        "react/jsx-indent-props": [2, 4],
        "react/jsx-key": 1,
        "react/jsx-max-props-per-line": [1, { "maximum": 2 }],
        "react/jsx-no-duplicate-props": 2,
        "react/jsx-no-undef": 2,
        "react/jsx-no-target-blank": 1,
        "react/jsx-pascal-case": 2,
        "react/jsx-uses-react": 2,
        "react/jsx-uses-vars": 2,
        "react/jsx-sort-props": [1, {
            "callbacksLast": false,
            "shorthandFirst": false,
            "ignoreCase": true
        }],
        "react/jsx-tag-spacing": [2, { "beforeSelfClosing": "always" }],
        "react/no-children-prop": 0,
        "react/no-danger": 0,
        "react/no-deprecated": 2,
        "react/no-did-mount-set-state": 0,
        "react/no-did-update-set-state": 0, 
        "react/no-direct-mutation-state": 2,
        "react/no-find-dom-node": 0,
        "react/no-multi-comp": 0,
        "react/no-unknown-property": 2, 
        "react/no-string-refs": 0,
        "react/prop-types": 0, 
        "react/react-in-jsx-scope": 0, 
        "react/require-render-return": 2,
        "react/self-closing-comp": 2, 
        "react/sort-comp": [0, {
            "order": [
                "lifecycle",
                "everything-else",
                "render",
                "/^render.+$/",
                "draw",
                "/^draw.+$/"
            ],
            "groups": {
                "lifecycle": [
                    "displayName",
                    "props",
                    "propTypes",
                    "state",
                    "defaultProps",
                    "contextTypes",
                    "childContextTypes",
                    "mixins",
                    "statics",
                    "static-methods",
                    "constructor",
                    "getDefaultProps",
                    "initialState",
                    "getInitialState",
                    "getChildContext",
                    "componentWillMount",
                    "componentDidMount",
                    "componentWillReceiveProps",
                    "shouldComponentUpdate",
                    "componentWillUpdate",
                    "componentDidUpdate",
                    "componentWillUnmount"
                ]
            }
        }],
        "react/sort-prop-types": [1, {
            "callbacksLast": false,
            "ignoreCase": true,
            "requiredFirst": false
        }]
    }
}